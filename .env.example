# Google OAuth Credentials
# Get these from: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-client-secret

# Google Drive Configuration
DRIVE_UPLOAD_ROOT=your-google-drive-folder-id

# Cloudflare Worker Configuration
CDN_BASE_URL=https://your-worker.workers.dev

# API Authentication
# Generate a secure random token: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
API_TOKENS=your-secret-api-token-here

# Cloudflare KV Namespace IDs
# Create these in Cloudflare dashboard: Workers & Pages > KV
KV_UPLOAD_SESSIONS_ID=your-kv-namespace-id-for-sessions
KV_STATS_ID=your-kv-namespace-id-for-stats
